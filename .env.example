# Database Configuration
DATABASE_URL=jdbc:postgresql://localhost:5432/your_db
DATABASE_USER=user
DATABASE_PASSWORD=password

# PostgreSQL Docker Configuration
POSTGRES_DB=your_db
POSTGRES_USER=user
POSTGRES_PASSWORD=password

# TIDAL API Configuration
TIDAL_CLIENT_ID=client_id_here
TIDAL_CLIENT_SECRET=secret_here
TIDAL_API_BASE_URL=https://openapi.tidal.com
TIDAL_AUTH_BASE_URL=https://auth.tidal.com
TIDAL_TOKEN_ENDPOINT=/v1/oauth2/token

# Sync Configuration
TIDAL_SEARCH_QUERY=best rock songs
TIDAL_TRACK_LIMIT=50
TIDAL_SYNC_ON_STARTUP=false
TIDAL_SYNC_SCHEDULED_ENABLED=false
TIDAL_SYNC_CRON=0 0 */1 * * *
